{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "liaforlax",
  "compatibility_date": "2025-05-24",
  "compatibility_flags": ["nodejs_compat"],
  "assets": {
    "binding": "ASSETS",
    "directory": "./dist"
  },
  "main": "dist/_worker.js/index.js",
  "observability": {
    "enabled": true
  },
  "routes": [
    {
      "pattern": "liaforlax.com",
      "custom_domain": true
    }
  ],
  "preview_urls": true
}
